name":null,"original":{"line":56,"column":5},"generated":{"line":70,"column":0}}],"sources":{"ts/engine/events/Controller.ts":"export class Controller {\r\n    eventKeys: {\r\n        [name: string]: string[]\r\n    }\r\n    \r\n    right: boolean\r\n    left: boolean\r\n    up: boolean\r\n    down: boolean\r\n    enter: boolean\r\n    \r\n    constructor() {\r\n        this.eventKeys = {\r\n            \"right\": [\"ArrowRight\", \"KeyD\"],\r\n            \"left\": [\"ArrowLeft\", \"KeyA\"],\r\n            \"up\": [\"ArrowUp\", \"KeyW\"],\r\n            \"down\": [\"ArrowDown\", \"KeyS\"],\r\n            \"enter\": [\"Enter\"],\r\n        };\r\n\r\n        this.right = false;\r\n        this.left = false;\r\n        this.up = false;\r\n        this.down = false;\r\n        this.enter = false;\r\n        \r\n        window.addEventListener(\"keydown\", e=> this.updateKey(e.code, true));\r\n        window.addEventListener(\"keyup\", e=> this.updateKey(e.code, false));\r\n    }\r\n\r\n    onKeyDown(name: string, func: ()=> void) {\r\n\r\n        window.addEventListener(\"keydown\", e=> {\r\n            this.mapKeys((ekey, key)=> {\r\n\r\n                if (e.code == name && key == name)\r\n                    func();\r\n\r\n            });\r\n        });\r\n\r\n    }\r\n\r\n    updateKey(code: string, enabled: boolean) {\r\n        this.mapKeys((ekey, key)=> {\r\n            if (code == key)\r\n                this[ekey] = enabled;\r\n        });\r\n    }\r\n    mapKeys(func: (ekey: string, key: string)=> void) {\r\n        Object.keys(this.eventKeys).map(ekey=> {\r\n            this.eventKeys[ekey].map(key=> {\r\n                func(ekey, key)\r\n            })\r\n        })\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"fff8104693440ff6664139fe4887eb2e","cacheData":{"env":{}}}