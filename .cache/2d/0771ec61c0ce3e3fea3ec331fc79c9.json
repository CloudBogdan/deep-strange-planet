es\";\r\nimport { Entity } from \"./Entity\";\r\nimport { Ore } from \"./Ore\";\r\n\r\nexport class Player extends Entity {\r\n    constructor() {\r\n        super(\"player\", \"player-stay\");\r\n    }\r\n\r\n    init(game: Game) {\r\n        super.init(game);\r\n\r\n        this.collider.width = 10 * config.SPRITE_SCALE;\r\n        this.collider.height = 10 * config.SPRITE_SCALE;\r\n        this.collider.offset = new Vector2(3, 0);\r\n\r\n        // Mine\r\n        // game.gamepad.onAnyKeyDown(name=> {\r\n        //     this.dig(name as Axes);\r\n        //     // console.log(name);\r\n        // });\r\n    }\r\n\r\n    dig(direction: Direction) {\r\n            \r\n        if (this.collider.collidesWith != null) {\r\n            const ore = this.game.getChildById(this.collider.collidesWith.id) as Ore;\r\n\r\n            if (!ore) return;\r\n\r\n            if (this.collider.collidesWith[direction] && this.position.distance(ore.position) < 90 && this.game.clock.elapsed % 16 == 0) {\r\n                ore.hit(2);\r\n\r\n                if (direction == \"right\")\r\n                    this.offset = new Vector2(10, 0);\r\n                else if (direction == \"left\")\r\n                    this.offset = new Vector2(-10, 0);\r\n                else if (direction == \"top\")\r\n                    this.offset = new Vector2(0, -10);\r\n                else if (direction == \"bottom\")\r\n                    this.offset = new Vector2(0, 10);\r\n            }\r\n\r\n        }\r\n\r\n    }\r\n\r\n    update() {\r\n        super.update();\r\n\r\n        this.movement.set((+this.game.gamepad.right - +this.game.gamepad.left), (+this.game.gamepad.down - +this.game.gamepad.up));\r\n        this.move();\r\n\r\n        if (this.movement.x != 0)\r\n            this.dig(this.movement.x > 0 ? \"right\" : \"left\");\r\n        else if (this.movement.y != 0)\r\n            this.dig(this.movement.y > 0 ? \"bottom\" : \"top\");\r\n\r\n        this.offset.lerp(Vector2.zero(), .2);\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"8767f53ec2360a71e39740ddd455468b","cacheData":{"env":{}}}